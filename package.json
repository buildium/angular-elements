{
  "name": "@buildium/angular-elements",
  "version": "1.2.0-2",
  "description": "A collection of directives for building angular applications",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "eslint src/",
    "test": "karma start --single-run",
    "compile": "babel src --out-dir lib/ --presets=es2015",
    "dist": "mkdirp dist && browserify lib/index.js -o dist/buildium-angular-elements.js",
    "build": "npm run compile && npm run dist",
    "prepublishOnly": "npm run build",
    "predocs": "npm run build",
    "docs": "node .build/docs.js",
    "postversion": "npm run docs -- --gh-pages",
    "watch": "nodemon --watch src/ --exec npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildium/angular-elements.git"
  },
  "keywords": [],
  "author": "Buildium <buidiumadmin@buildium.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildium/angular-elements/issues"
  },
  "homepage": "https://github.com/buildium/angular-elements#readme",
  "dependencies": {
    "lodash.uniqueid": "4.0.1"
  },
  "peerDependencies": {
    "angular": ">=1.5",
    "jquery": "^2.2.3"
  },
  "devDependencies": {
    "@buildium/angular-docs": "1.1.0",
    "@buildium/theme": "2.10.0-navigation.0",
    "angular": "1.5.9",
    "angular-mocks": "1.5.9",
    "babel-cli": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "browserify": "14.1.0",
    "electron": "1.6.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-config-buildium": "4.1.0-0",
    "eslint-plugin-buildium": "2.1.0",
    "eslint-plugin-import": "2.8.0",
    "faker": "4.1.0",
    "jasmine": "2.5.3",
    "jquery": "2.2.4",
    "karma": "1.5.0",
    "karma-browserify": "5.1.1",
    "karma-electron-launcher": "0.2.0",
    "karma-jasmine": "1.1.0",
    "mkdirp": "0.5.1",
    "nodemon": "1.14.12",
    "watchify": "3.9.0"
  },
  "eslintConfig": {
    "extends": "buildium",
    "rules": {
      "linebreak-style": [
        "warn",
        "unix"
      ]
    },
    "env": {
      "jasmine": true
    },
    "globals": {
      "angular": true
    }
  }
}
